{
  "name": "omegaalfa/treerouter",
  "type": "library",
  "license": "MIT",
  "description": "This library implements the Trie routing logic",
  "keywords": [
    "router", "routes", "routeTree", "TreeRoute", "NodeRoute",
    "Prefix Trees", "PSR", "PSR-7", "PSR-15", "HTTP",
    "message", "requests", "response", "middleware",
    "Laminas", "Diactoros", "php8"
  ],
  "authors": [
    {
      "name": "wrt",
      "email": "webdesenvolver.agenda@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "laminas/laminas-diactoros": "^2.17 || ^3.0",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Omegaalfa\\TreeRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omegaalfa\\TreeRouter\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0 || ^12.0",
    "phpstan/phpstan": "^1.11 || ^2.0"
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse src --level=6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
